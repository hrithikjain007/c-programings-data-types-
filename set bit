//wap to set nth bit of a number


#include<stdio.h>
void main()
{
  unsigned	int a,b=0,c,i,n;
	printf("\nenter the number\n");
	scanf("%d",&a);
		printf("\n enter the position\n");
	scanf("%d",&n);
	        for(i=31;i!=-1;i=i-1)
		
			
			printf("%d",a>>i&1);
		a=a|1<<n;
		printf("\n");
                 for(i=31;i!=-1;i=i-1)
			 printf("%d",a>>i&1);
                 
}
