#include<stdio.h>
void main()
{
	int x=10;
	static int i=200;
	register int r=300;
	 extern int a;
	printf("\nauto*****   X=%d",x);
          
	
		printf("\nstatic****   i=%d\n",i);

		printf("\nregister****    r=%d\n",r);

		printf("\nextern******  a=%d\n",a);
	{
		int x=20;
		static int i=300;
		register int r=500;
		printf("\nauto*****   X=%d\n",x);

		printf("\nstatic*** i=%d\n",i);

		printf("\nregister****      r=%d\n",r);

		
		
	}
	printf("\nauto***** X=%d\n",x);

		printf("\nstatic***** i=%d\n",i);

		printf("\nregister****** r=%d\n",r);
}
